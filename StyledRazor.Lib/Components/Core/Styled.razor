@(Type
    .Content(ChildContent)
    .Params(Params)
    .Render())

@code {

    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public Lib.Core.Styled Type { get; set; } = new();

    [Parameter(CaptureUnmatchedValues = true)]
    public IReadOnlyDictionary<string, object> Params { get; set; }

}