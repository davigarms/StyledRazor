@(Component
	.Content(ChildContent)
	.Params(Params)
	.Render())

@code {
	[Parameter]
	public RenderFragment ChildContent { get; set; }

	[Parameter]
	public Styled Component { get; set; } = new ("div");
	
	[Parameter(CaptureUnmatchedValues = true)]
	public IReadOnlyDictionary<string, object> Params { get; set; }
}